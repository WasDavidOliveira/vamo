{
  "version": 2,
  "framework": "angular",
  "buildCommand": "yarn build",
  "outputDirectory": "dist/vamo",
  "installCommand": "yarn install --frozen-lockfile",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/dist/vamo/index.html"
    }
  ]
}