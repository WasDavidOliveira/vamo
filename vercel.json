{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["dist/**"]
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/dist/vamo/index.html"
    }
  ],
  "buildCommand": "npm run build",
  "outputDirectory": "dist/vamo",
  "installCommand": "npm ci --cache /tmp/.npm --prefer-offline --no-audit",
  "framework": "angular"
}